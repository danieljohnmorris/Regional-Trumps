<h1>Editing game_type</h1>

<% form_for(@game_type, :url => admin_game_type_path(@game_type), :html => {:method => :put}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', admin_game_type_path(@game_type) %> |
<%= link_to 'Back', admin_game_types_path %>